<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kx.studyview">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ColorfulTextViewActivity" />
        <activity android:name=".activity.WaveViewActivity" />
        <activity android:name=".activity.SuperButtonActivity" />
        <activity android:name=".activity.TabLayoutActivity" />
        <activity android:name=".activity.PrinterTextViewActivity" />
        <activity android:name=".activity.PayInPutViewActivity" />
        <activity android:name=".activity.AnimationButtonActivity" />
        <activity android:name=".activity.RenderScriptActivity" />
        <activity android:name=".activity.LayoutContainerActivity" />
        <activity android:name=".activity.BusActivity" />
        <activity android:name=".activity.RegionActivity" />
        <activity android:name=".activity.PaintViewActivity"></activity>
    </application>

</manifest>